<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">

	<!-- DFORMS -->

	<map:pipelines>
		<map:pipeline type="noncaching">
		
			<!-- testing block inheritance -->
			<map:match pattern="test">
				<map:read src="block:/testsource" />
			</map:match>

			<map:match pattern="testsource">
				<map:read src="test.xml" />
			</map:match>
			
			<!-- testing block inheritance -->
			<map:match pattern="other">
				<map:read src="block:/othersource" />
			</map:match>

			<map:match pattern="othersource">
				<map:read src="test.xml" />
			</map:match>

		</map:pipeline>

	</map:pipelines>

</map:sitemap>

