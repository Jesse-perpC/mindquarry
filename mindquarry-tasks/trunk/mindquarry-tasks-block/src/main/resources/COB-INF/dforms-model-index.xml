<?xml version="1.0" encoding="utf-8"?>
<!-- A minimal collection file, only for use with
	 collection2resourceincludes.xsl. It is needed because the
	 TraversableGenerator, which would produce this when giving him the
	 "dforms-model" directory as input, does not work when deployed in tomcat and
	 the jndi:-protocol is used instead of the file:-protocol -->
<collection:collection
	xmlns:collection="http://apache.org/cocoon/collection/1.0" name="dforms-model">
	<collection:resource name="archived.xml" />
	<collection:resource name="date.xml" />
	<collection:resource name="description.xml" />
	<collection:resource name="people.xml" />
	<collection:resource name="status.xml" />
	<collection:resource name="summary.xml" />
</collection:collection>