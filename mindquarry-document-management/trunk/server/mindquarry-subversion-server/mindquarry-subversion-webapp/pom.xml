<?xml version="1.0" encoding="UTF-8"?><project>
  <parent>
    <artifactId>mindquarry-subversion-server</artifactId>
    <groupId>com.mindquarry.document-management.server</groupId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.mindquarry.document-management.server</groupId>
  <artifactId>mindquarry-subversion-webapp</artifactId>
  <packaging>war</packaging>
  <name>mindquarry-subversion-webapp</name>
  <version>1.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <groupId>org.mortbay.jetty</groupId>
        <artifactId>maven-jetty-plugin</artifactId>
        <version>6.0.2</version>
        <configuration>
          <connectors>
            <connector implementation="org.mortbay.jetty.nio.SelectChannelConnector">
              <port>8888</port>
              <maxIdleTime>30000</maxIdleTime>
            </connector>
          </connectors>
          <webAppSourceDirectory>${project.build.directory}/${pom.artifactId}-${pom.version}</webAppSourceDirectory>
          <contextPath>/</contextPath>
        </configuration>
      </plugin>
    </plugins>
  </build>

<dependencies>
      <dependency>
        <groupId>com.mindquarry.document-management.server</groupId>
        <artifactId>mindquarry-subversion-block</artifactId>
        <version>1.0-SNAPSHOT</version>
      </dependency>
    </dependencies>

  <repositories>
    <repository>
      <releases />
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>apache.snapshot</id>
      <name>Apache Snapshot Repository</name>
      <url>http://people.apache.org/repo/m2-snapshot-repository</url>
    </repository>
    <repository>
      <id>cocoon.staging</id>
      <name>Cocoon staging repository</name>
      <url>http://people.apache.org/builds/cocoon</url>
    </repository>
  </repositories>
</project>
