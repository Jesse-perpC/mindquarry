<sitemap xmlns="http://apache.org/cocoon/sitemap/1.0">
	<components>
		<actions>
			<action name="log" src="com.mindquarry.webapp.acting.DebugLogAction"/>
			<action name="log-params" src="com.mindquarry.webapp.acting.LogParametersAction"/>
			<action name="authorise" src="com.mindquarry.webapp.acting.AuthorisationAction"/>
		</actions>

		<generators default="file">
			<generator name="jsp" src="org.apache.cocoon.generation.JSPGenerator"/>
		</generators>
	</components>

	<pipelines>
		<pipeline type="noncaching">
			<match pattern="">
				<generate src="admin/index.jsp" type="jsp"/>
				<transform type="log" />
				<serialize type="html"/>
			</match>

			<match pattern="*.jsp">
				<generate src="admin/{1}.jsp" type="jsp"/>
				<transform type="log" />
				<serialize type="html"/>
			</match>

			<match pattern="*.png">
				<read src="admin/{1}.png"/>
			</match>

			<match pattern="*.gif">
				<read src="admin/{1}.gif"/>
			</match>

			<match pattern="*.css">
				<read src="admin/{1}.css"/>
			</match>
		</pipeline>
	</pipelines>
</sitemap>
