<xb:config
	xmlns:xb="http://xml.apache.org/xmlbeans/2004/02/xbean/config">

	<xb:namespace
		uri="http://www.mindquarry.com/ns/schema/conversation">
		<xb:package>com.mindquarry.types.conversation</xb:package>
	</xb:namespace>

	<xb:extension
		for="com.mindquarry.types.conversation.ContributionReference">
		<xb:interface
			name="com.mindquarry.types.conversation.ContributionLogic">
			<xb:staticHandler>
				com.mindquarry.types.conversation.ContributionHandler
			</xb:staticHandler>
		</xb:interface>
	</xb:extension>
</xb:config>
