<?xml version="1.0" encoding="UTF-8"?>
<configuration
	xmlns="http://www.mindquarry.com/ns/schema/xmlbeans-persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mindquarry.com/ns/schema/xmlbeans-persistence ../../../../../../main/xsd/xmlbeans-persistence.xsd ">
	<entity>
		<className>com.mindquarry.types.user.User</className>
		<path>/users</path>
	</entity>
	<entity>
		<className>com.mindquarry.types.teamspace.Teamspace</className>
		<path>/teamspaces</path>
	</entity>
	<queryInfo>
		<key>getUserById</key>
		<query>/users/{$userId}</query>
		<resultEntityClass>com.mindquarry.types.user.User</resultEntityClass>
	</queryInfo>
	<queryInfo>
		<key>getAllUsers</key>
		<query>/users</query>
		<resultEntityClass>com.mindquarry.types.user.User</resultEntityClass>
	</queryInfo>
</configuration>
