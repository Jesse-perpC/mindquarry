<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<packaging>jar</packaging>

	<parent>
		<groupId>com.mindquarry.document-management.server</groupId>
		<artifactId>mindquarry-document-management-server</artifactId>
		<version>1.1-beta</version>
	</parent>

	<name>Mindquarry Document Management Server Xenodot</name>
	<groupId>com.mindquarry.document-management.server.xenodot</groupId>
	<artifactId>mindquarry-document-management-server-xenodot</artifactId>

	<description>
		Dropin replacement for JackRabbit
	</description>
	
	<dependencies>
		<dependency>
			<groupId>org.apache.jackrabbit</groupId>
			<artifactId>jackrabbit-core</artifactId>
			<version>1.2.3</version>
			<exclusions>
				<exclusion>
					<artifactId>jcr</artifactId>
					<groupId>jsr170</groupId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<artifactId>jcr</artifactId>
			<groupId>javax.jcr</groupId>
			<version>1.0.1</version>
		</dependency>
		<dependency>
		    <groupId>postgresql</groupId>
		    <artifactId>postgresql</artifactId>
		    <version>8.2-504.jdbc3</version>
		</dependency>
		<dependency>
			<groupId>jaxen</groupId>
			<artifactId>jaxen</artifactId>
			<version>1.1-beta-9</version>
		</dependency>
	</dependencies>

	<scm>
		<developerConnection>scm:svn:https://www.mindquarry.org/repos/mindquarry-document-management/tags//release-com.mindquarry.document-management.server.xenodot-mindquarry-document-management-server-xenodot-1.1-beta</developerConnection>
	</scm>
</project>